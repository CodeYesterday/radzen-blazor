@inherits RadzenComponent

@if (Visible)
{
    if (IconX != null)
    {
        @IconX.RenderIcon(getStyle(), GetCssClass(), GetId(), Attributes)
    }
    else
    {
        <i @ref="@Element" style="@getStyle()" @attributes="Attributes" class="@GetCssClass()" id="@GetId()">@((MarkupString)Icon)</i>
    }
}
